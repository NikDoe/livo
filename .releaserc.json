{
	"repositoryUrl": "https://github.com/NikDoe/livo.git",
	"branches": [
		"main"
	],
	"plugins": [
		"@semantic-release/commit-analyzer",
		"@semantic-release/release-notes-generator",
		"@semantic-release/changelog",
		[
			"@semantic-release/git",
			{
				"assets": [
					"CHANGELOG.md",
					"package.json"
				],
				"message": "chore(release): ${nextRelease.version} [skip ci]",
				"annotateCommits": false
			}
		],
		[
			"@semantic-release/github",
			{
				"assets": [
					"CHANGELOG.md"
				],
				"successComment": "Release ${nextRelease.version} is published! ðŸŽ‰",
				"failComment": "Failed to publish release ${nextRelease.version}.",
				"draft": false
			}
		]
	]
}